{
  "applicationId": "js.neutralino.svelte",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/svelte-app/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*", "events.*", "window.*", "debug.*"],
  "modes": {
    "window": {
      "enableInspector": true,
      "title": "neutralinojs-svelte",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/svelte-app/public/icon.png",
      "center": true,
      "resizable": true,
      "injectGlobals": true
    }
  },
  "cli": {
    "binaryName": "neutralinojs-svelte",
    "resourcesPath": "/svelte-app/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "5.5.0",
    "clientVersion": "5.5.0",

    "frontendLibrary": {
      "patchFile": "/svelte-app/dist/index.html",
      "devUrl": "http://localhost:5173", 
      "projectPath": "/svelte-app/", 
      "initCommand": "npm install", 
      "devCommand": "npm run dev", 
      "buildCommand": "npm run build"
    }
  }
}
